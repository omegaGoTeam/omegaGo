<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:converters="using:OmegaGo.UI.WindowsUniversal.Converters">

    <converters:NativeLocalizingConverter x:Key="LocalizingConverter" />
    <converters:NativeEnumLocalizingConverter x:Key="EnumLocalizingConverter" />
    <converters:NativeVisibilityConverter x:Key="VisibilityConverter" />
    <converters:NativeInvertedVisibilityConverter x:Key="InvertedVisibilityConverter" />
    <converters:NativeConcreteEnumLocalizingConverter x:Key="ConcreteEnumLocalizingConverter" />
    <converters:NativeTypeNameLocalizingConverter x:Key="TypeNameLocalizingConverter" />
    <converters:NativeEnumSelectionConverter x:Key="EnumSelectionConverter" />
    <converters:NativeBoolNegationConverter x:Key="BoolNegationConverter" />
    <converters:ItemClickEventArgsConverter x:Key="ItemClickEventArgsConverter" />
    <converters:NativeStringToUpperConverter x:Key="StringToUpperConverter" />
    <converters:NativeEmptyCollectionVisibilityConverter x:Key="EmptyCollectionVisibilityConverter" />
    <converters:NativeNonEmptyStringVisibilityConverter x:Key="NonEmptyStringVisibilityConverter" />
    <converters:NativePluralizingConverter x:Key="PluralizingConverter" />
</ResourceDictionary>
