<UserControl
    x:Class="OmegaGo.UI.WindowsUniversal.UserControls.Navigation.BackButton"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Width="48"
    Height="32"
    d:DesignHeight="300"
    d:DesignWidth="400">
    <Button HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Style="{StaticResource TitleBarBackButtonStyle}"  FontFamily="Segoe MDL2 Assets" Click="{x:Bind BackRequested}">
        <Viewbox>
            <SymbolIcon Symbol="Back" />
        </Viewbox>
    </Button>
</UserControl>